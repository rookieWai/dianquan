<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph_mine"
    app:startDestination="@id/mineFragment">
    <fragment
        android:id="@+id/mineFragment"
        android:name="com.wei.mine.ui.MineFragment"
        tools:layout="@layout/fragment_mine"
        android:label="MineFragment" >
        <action
            android:id="@+id/action_mineFragment_to_menuFragment"
            app:destination="@id/menuFragment" />
        <action
            android:id="@+id/action_mineFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_mineFragment_to_userInfoFragment"
            app:destination="@id/userInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/userInfoFragment"
        android:name="com.wei.mine.ui.UserInfoFragment"
        android:label="UserInfoFragment" />
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.wei.mine.ui.MenuFragment"
        android:label="MenuFragment" />
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.wei.mine.ui.OrderFragment"
        android:label="OrderFragment" />
</navigation>